{
  "name": "ata-api",
  "version": "1.0.0",
  "type": "module",
  "description": "",
  "main": "index.js",
  "engines": {
    "node": "21.7.1"
  },
  "scripts": {
    "start": "node ./src/server.js",
    "dev": "npx nodemon -r dotenv/config --inspect ./src/server.js",
    "db:migrate": "node ./src/database/migrations/$npm_config_name",
    "db:create": "node ./src/database/migrations/index.js",
    "seed": "node ./src/shared/seeders/insertDataChurchMembersTable.js",
    "setup": "npm install && npm run db:create && npm run seed",
    "clean": "rm -rf build && mkdir build",
    "build": "npm run clean && npx babel src --ignore=src/test --out-dir=build -s"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/core": "^7.24.3",
    "@babel/node": "^7.23.9",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/preset-env": "^7.24.3",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.0",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "express-validator": "^7.0.1",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.11.3"
  },
  "devDependencies": {
    "@babel/plugin-proposal-optional-chaining": "^7.21.0",
    "@babel/plugin-transform-optional-chaining": "^7.24.1",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.6",
